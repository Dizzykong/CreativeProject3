AAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,KAAA,CAAA,WAAA,GAAA,UAAA,CAAA,KAAA,CAAA,SAAA,CAAA,WAAA,EAAA,CAAA;AAAA,OAAA,CAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA;AAAA,eAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AAAA,OAAA,CAAA;AACA;AAJA,GAfA;AAqBA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KAJA;AAKA,IAAA,UALA,sBAKA,IALA,EAKA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,iDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAdA;AAeA,IAAA,QAfA,oBAeA,IAfA,EAeA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAKA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AANA,kDAOA,IAPA;;AAAA;AAAA;AAAA;AASA,gBAAA,OAAA,CAAA,GAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KA1BA;AA2BA,IAAA,QA3BA,sBA2BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KAnCA;AAoCA,IAAA,WApCA,uBAoCA,KApCA,EAoCA;AACA,WAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAtCA;AAuCA,IAAA,MAvCA,oBAuCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,QAFA,GAEA,IAAA,QAAA,EAFA;AAGA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAAA;AAAA,uBAKA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA;AAFA,iBAAA,CALA;;AAAA;AAKA,gBAAA,EALA;AASA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AATA;AAAA;;AAAA;AAAA;AAAA;AAWA,gBAAA,OAAA,CAAA,GAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA;AApDA;AArBA,CAAA","sourcesContent":["<template>\n<div class=\"admin\">\n  <div class=\"px-4 py-5 my-5 text-center\">\n    <h1 class=\"display-5 fw-bold\">Add More!</h1>\n    <div class=\"col-lg-6 mx-auto\">\n      <p class=\"lead mb-4\">Add an Animal</p>\n      <input v-model=\"title\" placeholder=\"How Fast is it? (in mph)\"><br>\n      <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n      <button @click=\"upload\">Upload</button> <br>\n      <div class=\"upload\" v-if=\"addItem\">\n        <h2>{{addItem.title}}</h2>\n        <img :src=\"addItem.path\" />\n      </div>\n    </div>\n  </div>\n  <div class=\"px-4 py-5 my-5 text-center\">\n    <h1 class=\"display-5 fw-bold\">Edit or Delete an Animal from the list</h1>\n    <div class=\"col-lg-6 mx-auto\">\n      <div class=\"form\">\n        <input v-model=\"findTitle\" placeholder=\"Search\">\n        <div v-if=\"suggestions.length > 0\">\n          <div v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}\n          </div>\n        </div>\n      </div>\n      <div v-if=\"findItem\">\n        <input v-model=\"findItem.title\">\n        <img :src=\"findItem.path\" />\n      </div>\n      <div v-if=\"findItem\">\n        <button @click=\"deleteItem(findItem)\">Delete</button>\n        <button @click=\"editItem(findItem)\">Edit</button>\n      </div>\n    </div>\n  </div>\n</div>\n</template>\n\n<style scoped>\n.image h2 {\n  font-style: italic;\n  font-size: 1em;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h2 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit {\n  display: flex;\n}\n\n.circle {\n  border-radius: 50%;\n  width: 18px;\n  height: 18px;\n  padding: 8px;\n  background: #333;\n  color: #fff;\n  text-align: center\n}\n\n/* Form */\ninput,\ntextarea,\nselect,\nbutton {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1em;\n}\n\n.form {\n  margin-right: 50px;\n}\n\n/* Uploaded images */\n.upload h2 {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n}\n\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5BDEFF;\n  color: #fff;\n}\n</style>\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'Admin',\n  data() {\n    return {\n      title: \"\",\n      file: null,\n      addItem: null,\n      items: [],\n      findTitle: \"\",\n      findItem: null,\n    }\n  },\n  created() {\n    this.getItems();\n  },\n  computed: {\n    suggestions() {\n      let items = this.items.filter(item => item.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\n      return items.sort((a, b) => a.title > b.title);\n    }\n  },\n  methods: {\n    selectItem(item) {\n      this.findTitle = \"\";\n      this.findItem = item;\n    },\n    async deleteItem(item) {\n      try {\n        await axios.delete(\"/api/items/\" + item._id);\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async editItem(item) {\n      try {\n        await axios.put(\"/api/items/\" + item._id, {\n          title: this.findItem.title,\n        });\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async getItems() {\n      try {\n        let response = await axios.get(\"/api/items\");\n        this.items = response.data;\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    fileChanged(event) {\n      this.file = event.target.files[0]\n    },\n    async upload() {\n      try {\n        const formData = new FormData();\n        formData.append('photo', this.file, this.file.name)\n        let r1 = await axios.post('/api/photos', formData);\n        let r2 = await axios.post('/api/items', {\n          title: this.title,\n          path: r1.data.path\n        });\n        this.addItem = r2.data;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n}\n</script>"],"sourceRoot":"src/views"}]}